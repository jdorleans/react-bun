    "react-dom": "^19",
    "react-geocode": "^2.0.1",
    "react-router": "^7.6.1",
    "react-router-dom": "^7.6.1",
    "react-toastify": "^11.0.5",
    "typeorm": "^0.3.24"
  },
  "devDependencies": {
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/bun": "latest"
